IgnoreUnknown UseKeychain

Host githubnmr
     #LogLevel DEBUG3
     HostName github.com
     PreferredAuthentications publickey
     PubkeyAuthentication yes
     IdentitiesOnly yes
     IdentityFile ~/.ssh/nmr_id_rsa

Host *.*
  #LogLevel DEBUG3
  UseKeychain yes
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_rsa

#[url "https://github.com"]
#    insteadOf = "git@github.com:"
